{
  "name": "ldapts",
  "version": "1.4.0",
  "description": "LDAP client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "mocha -r ts-node/register tests/**/*.tests.ts",
    "lint": "tslint --fix -p tsconfig.lint.json -c tslint.json -t stylish",
    "prepublishOnly": "npm run build && npm run lint && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ldapts/ldapts.git"
  },
  "engines": {
    "node": ">=8.11"
  },
  "keywords": [
    "ldap",
    "active",
    "directory"
  ],
  "author": "jim@biacreations.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ldapts/ldapts/issues"
  },
  "homepage": "https://github.com/ldapts/ldapts#readme",
  "dependencies": {
    "asn1": "~0.2.4",
    "debug": "~4.1.0",
    "strict-event-emitter-types": "~2.0.0"
  },
  "devDependencies": {
    "@types/asn1": "~0.2.0",
    "@types/chai": "~4.1.7",
    "@types/chai-as-promised": "~7.1.0",
    "@types/debug": "0.0.31",
    "@types/mocha": "~5.2.5",
    "@types/node": "~10.12.15",
    "chai": "~4.2.0",
    "chai-as-promised": "~7.1.1",
    "husky": "~1.2.1",
    "mocha": "~5.2.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "tslint-config-airbnb": "~5.11.1",
    "typescript": "~3.2.2"
  }
}
